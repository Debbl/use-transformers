{
  "name": "use-transformers",
  "type": "module",
  "version": "0.0.9",
  "packageManager": "pnpm@9.15.0",
  "description": "Easily use transformers.js with react in browser",
  "author": "Brendan Dash <me@aiwan.run> (https://aiwan.run)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/use-transformers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Debbl/use-transformers.git"
  },
  "bugs": "https://github.com/Debbl/use-transformers/issues",
  "keywords": [
    "transformers",
    "transformers.js",
    "react",
    "react-hooks",
    "web-worker"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "release": "pnpm build && bumpp && pnpm publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@huggingface/transformers": "^3.1.2",
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "dependencies": {
    "birpc": "^0.2.19",
    "use-rpc": "^0.0.3",
    "use-rpc-worker": "^0.0.3"
  },
  "devDependencies": {
    "@debbl/eslint-config": "^3.8.5",
    "@huggingface/transformers": "^3.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "bumpp": "^9.9.1",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
